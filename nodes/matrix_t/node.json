{
  "name": "matrix_t",
  "ns": "jsfeat",
  "description": "Matrix Type",
  "phrases": {
    "active": "Creating matrix"
  },
  "ports": {
    "input": {
      "cols": {
        "title": "Cols",
        "type": "number"
      },
      "rows": {
        "title": "Rows",
        "type": "number"
      },
      "type": {
        "title": "Type",
        "type": "string",
        "enum": ["U8_t | C1_t", "F32C1_t", "U8C1_t"]
      }
    },
    "output": {
      "out": {
        "title": "Matrix",
        "type": "matrix_t"
      }
    }
  },
  "dependencies": {
    "npm": {
      "jsfeat": "latest"
    }
  }
}
