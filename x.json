{
  "name": "http",
  "description": "Jsfeat nodes for ChiÏ‡",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodule/jsfeat.git"
  },
  "dependencies": {
    "jsfeat": "0.0.8"
  },
  "nodes": [
    {
      "dependencies": {
        "npm": {
          "jsfeat": "latest"
        }
      },
      "ports": {
        "input": {
          "src": {
            "title": "Source",
            "type": "any"
          },
          "dest": {
            "title": "Destination",
            "async": true,
            "type": "any"
          },
          "kernel_size": {
            "title": "Kernel Size",
            "type": "number"
          },
          "sigma": {
            "title": "Sigma",
            "type": "number"
          }
        },
        "output": {
          "out": {
            "title": "Out",
            "type": "any"
          }
        }
      },
      "phrases": {
        "active": "Applying gaussian blur"
      },
      "description": "Gaussian blur",
      "async": true,
      "ns": "jsfeat",
      "name": "gaussian_blur",
      "fn": "on.input.dest = function() {\n  jsfeat.gaussian_blur($.src, $.dest, $.kernel_size, $.sigma)\n  \n  output( { out: $.write('dest', $.dest) } );\n};\n"
    },
